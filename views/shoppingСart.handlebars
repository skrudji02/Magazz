 <h1>Shopping Cart</h1>  
   {{#each basket}}
       <div class='col-6' data-id='{{this.id}}'>
          <div class="p-2 border " >
                <img class = "picture" src="{{this.photo}}" alt="picture">
                <h1 id="name">{{this.name_guitar}}, {{this.price}}</h1>
                <button  data-id="{{this.id}}" class='button_delete'>Удалить</button>           
          </div>                  
       </div>   
   {{/each}}
            
  