
  <div class="container text-center">
    <div class="row g-2">
      {{#each guitars}}
       <div class="col-6">
          <img class="picture" src="{{this.photo}}" alt="picture">
          <div class="p-3 border bg-light" onclick="openWindow({{this.id}})">{{this.name_guitar}}</div>
          <script>
            function openWindow(id){
              window.open("http://localhost:3000/magazz/guitar/"+id, "_blank");
            }
          </script>             
       </div>     
      {{/each}}
    </div>
  </div>
</div>

